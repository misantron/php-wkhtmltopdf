{
  "name": "mediatech/php-wkhtmltopdf",
  "description": "A PHP wrapper interface to wkhtmltopdf command tool",
  "keywords": ["pdf", "pdf converter", "html to pdf", "wkhtmltopdf"],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "mockery/mockery": "~0.9"
  },
  "suggest": {
    "h4cc/wkhtmltopdf-i386": "*",
    "h4cc/wkhtmltopdf-amd64": "*"
  },
  "autoload": {
    "psr-4": {
      "MediaTech\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MediaTech\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}